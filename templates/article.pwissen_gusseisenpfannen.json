      "type": "rich-text",
      "blocks": {
        "heading_6LNRtU": {
          "type": "heading",
          "settings": {
            "text": "Warum Kochgeschirr aus Gusseisen in jede Küche gehört",
            "heading_tag": "h2",
            "text_color": "",
